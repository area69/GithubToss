@* Subsidiary Ledger Class Panel *@
<div class="card-body bg-fields">
    <div class="row">
        <div class="col-xl-2 col-lg-1 col-md-0"></div>
        <div class="col-xl-5 col-lg-6 col-md-8">
            <div class="form-group">
                <label><b>Subsidiary Ledger Class</b></label>
                <input type="text" id="" value="" class="form-control">
            </div>
        </div>
        <div class="col-xl-3 col-lg-4 col-md-4">
            <div class="form-group">
                <label><b>Subsidiary Ledger Code</b></label>
                <input type="text" id="" value="" class="form-control">
            </div>
        </div>

    </div>
    <div class="row">
        <div class="col-xl-2 col-lg-1 col-md-0"></div>
        <div class="col-xl-3 col-lg-4 col-md-5">
            <div class="form-group">
                <label><b>Fund</b></label>
                <select class="form-control select2">
                    <option>General Fund</option>
                    <option>Calamity Fund</option>
                    <option>Development Fund</option>
                    <option>Special Education Fund</option>
                    <option>Trust Fund</option>
                </select>
            </div>
        </div>
        <div class="col-xl-5 col-lg-6 col-md-7">
            <div class="form-group">
                <label><b>General Ledger Account</b></label>
                <select class="form-control select2">
                    <option value="">Accountable Forms Expenses</option>
                    <option value="">Accountable Forms Inventory</option>
                    <option value="">Accounts Payable</option>
                    <option value="">Accounts Receivable</option>
                    <option value="">Accumulated Depreciation - Agricultural, Fishery and Forestry Equipment</option>
                    <option value="">Accumulated Depreciation - Airport Equipment</option>
                    <option value="">Accumulated Depreciation - Communication Equipment</option>
                </select>
            </div>

        </div>
    </div>
    <div class="row">
        <div class="col-12">
            <div class="form-group">
                <button type="button" class="float-right btn btn-danger btn-min-width" onclick="changeBtn2('btnAddSubLedger')">Cancel</button>
                <button type="button" id="btnAddSubLedger" class="float-right btn btn-info btn-min-width mr-1 popover-trigger" onfocusout='PopoverLostFocus()'>Add</button>
            </div>
        </div>
    </div>
</div>
<div class="card-body box-shadow-1">
    <div class="row mt-1">
        <div class="col-12">
            <table class="table table-striped table-bordered table-condensed zero-configuration w-100 table-smss-hover">
                <thead class="text-center">
                    <tr>
                        <th>Subsidiary Ledger Class</th>
                        <th>Subsidiary Ledger Code</th>
                        <th>Fund</th>
                        <th>General Account</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                    @{
                        var DeleteConfirmationMess = "Are you sure do you want to delete this?";
                        var DeleteSuccesMess = "Successfully Deleted";

                        var btnEdit = "<button class='btn btn-icon btn-success btn-block  border-0' id='' onclick=\"changeBtn('btnAddSubLedger')\"><i class='ft-edit'></i> Edit</button>";
                        var btnDelete = "<button class='btn btn-icon btn-danger white btn-block border-0' id='' onclick=\"swalConfirmation('" + @DeleteConfirmationMess + "', '', '" + @DeleteSuccesMess + "')\"><i class='la la-trash'></i> Delete</button>";
                        var btnQty = btnEdit + btnDelete;

                        <tr>
                            <td>GF - CASH IN BANK - LOCAL CURRENCY, CURRENT ACCOUNT</td>
                            <td>GF - CIBLCCA</td>
                            <td>General Fund</td>
                            <td>Cash in Bank - Local Currency, Current Account</td>
                            <td class="text-center">
                                <button type="button" onfocusout="PopoverLostFocus()" class="btn btn-sm btn-icon btn-btnbackcolor popover-trigger" data-toggle="popover" data-content="@btnQty" data-original-title="" data-html="true" data-placement="left">
                                    <i class="la la-bars white"></i>
                                </button>
                            </td>
                        </tr>
                        <tr>
                            <td>GF - CASH IN BANK - LOCAL CURRENCY, SAVINGS ACCOUNT</td>
                            <td>GF - CIBLCCA</td>
                            <td>General Fund</td>
                            <td>Cash in Bank - Local Currency, Saving Account</td>
                            <td class="text-center">
                                <button type="button" onfocusout="PopoverLostFocus()" class="btn btn-sm btn-icon btn-btnbackcolor popover-trigger" data-toggle="popover" data-content="@btnQty" data-original-title="" data-html="true" data-placement="left">
                                    <i class="la la-bars white"></i>
                                </button>
                            </td>
                        </tr>
                        <tr>
                            <td>SEF - DUE TO BIR</td>
                            <td>SEF - DB</td>
                            <td>Special Education Fund</td>
                            <td>Due to BIR</td>
                            <td class="text-center">
                                <button type="button" onfocusout="PopoverLostFocus()" class="btn btn-sm btn-icon btn-btnbackcolor popover-trigger" data-toggle="popover" data-content="@btnQty" data-original-title="" data-html="true" data-placement="left">
                                    <i class="la la-bars white"></i>
                                </button>
                            </td>
                        </tr>
                        <tr>
                            <td>SEF - DUE TO PAG-IBIG</td>
                            <td>SEF - DPI</td>
                            <td>Special Education Fund</td>
                            <td>Due to Pag-ibig</td>
                            <td class="text-center">
                                <button type="button" onfocusout="PopoverLostFocus()" class="btn btn-sm btn-icon btn-btnbackcolor popover-trigger" data-toggle="popover" data-content="@btnQty" data-original-title="" data-html="true" data-placement="left">
                                    <i class="la la-bars white"></i>
                                </button>
                            </td>
                        </tr>
                        <tr>
                            <td>TF - DUE TO NATIONAL GOVERNMENT AGENCIES</td>
                            <td>TF - DNGA</td>
                            <td>Trust Fund</td>
                            <td>Due to National Government Agencies</td>
                            <td class="text-center">
                                <button type="button" onfocusout="PopoverLostFocus()" class="btn btn-sm btn-icon btn-btnbackcolor popover-trigger" data-toggle="popover" data-content="@btnQty" data-original-title="" data-html="true" data-placement="left">
                                    <i class="la la-bars white"></i>
                                </button>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>

