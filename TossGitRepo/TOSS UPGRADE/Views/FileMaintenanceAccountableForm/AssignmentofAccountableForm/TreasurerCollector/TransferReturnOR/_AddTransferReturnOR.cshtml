                <div class="col-md-3 form-group" id="_DynamicSubCollectorID">
                </div>
                <div class="col-md-3 form-group" id="_DynamicStubNoID">
                </div>
                <div class="col-md-3 form-group">
                    <label class="mt-1">Date Transferred</label>
                    @Html.TextBoxFor(m => m.getAFTransferReturnORAssigncolumns.DateTransferred, new { @class = "form-control pickadate ui-datepicker", type = "text" })
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-1"></div>
        <div class="col-md-10" id="_TransferReturnORIDPV">
        </div>
    </div>
    <div class="row">
        <div class="col-md-10"></div>
        <div class="col-md-2">
            <button class="btn bg-addbutton form-control black" type="submit"><b>Assign</b></button>
        </div>
    </div>
}
<script>
    $(function () {
        LoadDataTable();
        LoadPopOver();
        LoadPickDatetime();
    })
</script>
