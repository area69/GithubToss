@model TOSS_UPGRADE.Models.FM_AccountableForm.FM_AccountableFormAssignment
@{
    DB_TOSSEntities SISDB = new DB_TOSSEntities();
}
@using TOSS_UPGRADE.Models;
<div class="row">
    <div class="col-md-3 form-group">
        <label class="mt-1">OR Description</label>
        @Html.TextBoxFor(m => m.AccountableTCTROROrDescID, new { @class = "form-control", type = "text", @disabled = "disabled" })
    </div>
    <div class="col-md-3 form-group">
        <label class="mt-1">Quantity</label>
        @Html.TextBoxFor(m => m.AccountableTCTRORQuantityID, new { @class = "form-control", type = "text", @disabled = "disabled" })
    </div>
    <div class="col-md-3 form-group">
        <label class="mt-1">Starting OR</label>
        @Html.TextBoxFor(m => m.AccountableTCTRORStartingORID, new { @class = "form-control", type = "text", @disabled = "disabled" })
    </div>
    <div class="col-md-3 form-group">
        <label class="mt-1">Ending OR</label>
        @Html.TextBoxFor(m => m.AccountableTCTROREndingORID, new { @class = "form-control", type = "text", @disabled = "disabled" })
    </div>
</div>