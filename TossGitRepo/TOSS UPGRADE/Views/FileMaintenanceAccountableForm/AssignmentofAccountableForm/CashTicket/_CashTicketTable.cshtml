@model IEnumerable<TOSS_UPGRADE.Models.FM_AccountableForm.AccountableFormAssignmentList>
@{
    DB_TOSSEntities SISDB = new DB_TOSSEntities();
}
@using TOSS_UPGRADE.Models;
<div class="row">
    <div class="col-md-12">
        <table class="table table-responsive table-striped zero-configuration table-bordered text-center scroll-horizontal table-smss-hover no-footer m-0 w-100">
            <thead>
                <tr>
                    <th>Collector</th>
                    <th>CT Type</th>
                    <th>Stub No.</th>
                    <th>Starting CT</th>
                    <th>Ending CT</th>
                    <th>Quantity</th>
                    <th>Amount</th>
                    <th>Consumed?</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody>
                @{
                    foreach (var Items in Model)
                    {
                        var DeleteConfirmationMess = "Are you sure do you want to delete this?";
                        var DeleteSuccesMess = "Successfully Deleted";
                <tr>
                    <td>@Items.CollectorName</td>
                    <td>
                        @{
                            var SubFundID = Items.SubFundID;
                            var SubFund = (from e in SISDB.SubFunds where e.SubFundID == SubFundID select e).FirstOrDefault();
                            var TDValueSubFund = "";
                            if (SubFund == null)
                            {
                                TDValueSubFund = "N/A";
                            }
                            else
                            {
                                TDValueSubFund = "(" + SubFund.Fund.FundName +")";
                            }
                        }
                        @TDValueSubFund - @Items.FieldFee
                    </td>
                    <td>@Items.StubNo</td>
                    <td>@Items.StratingOR</td>
                    <td>@Items.EndingOR</td>
                    <td>@Items.Quantity</td>
                    <td class="text-right">@Items.TotalAmount</td>
                    <td>@Items.isConsumed</td>
                    <td>
                        <button class="btn btn-sm btn-block black bg-headerbc border-0" title="Remove this OR" type="button" onclick="swalConfirmation('@DeleteConfirmationMess','','@DeleteSuccesMess', 'AngularGlobalDelete(@Items.AssignAFID ,&quot;DeleteAccountableFormAssignCashTicket&quot;,&quot;FMAccountableFormController&quot;)')"><i class='la la-remove - square'></i></button>
                    </td>
                </tr>
                    }
                }
            </tbody>
        </table>
    </div>
</div>

<script>
    $(function () {
        LoadDataTable();
        LoadPopOver();
    })
</script>

