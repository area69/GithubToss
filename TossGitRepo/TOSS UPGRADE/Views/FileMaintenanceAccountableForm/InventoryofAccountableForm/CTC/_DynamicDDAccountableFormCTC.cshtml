@model TOSS_UPGRADE.Models.FM_AccountableForm.FM_AccountableFormInventory
@{
    DB_TOSSEntities TOSSDB = new DB_TOSSEntities();
}
@using TOSS_UPGRADE.Models;

<label>Accountable Form</label>
@Html.DropDownListFor(m => m.AccountableFormInvtID, Model.AccountableFormInvtList, new { @class = "form-control select2", id = "AFIDDDCTCCTC", onchange = "LoadQTYVal()" })






<script>
    $(function () {
        LoadQTYVal();
    })
    function LoadQTYVal() {
        var AccFormDD = $("#AFIDDDCTCCTC").val();
        $.ajax({
            url: "/FileMaintenanceAccountableForm/GetQTYVal",
            data: { AccFormDD: AccFormDD },
            success: function (data) {
                $("#txtORInvQuantityValueCTC").val(data);
            }

        })
    }

</script>