@model TOSS_UPGRADE.Models.FM_AccountableForm.FM_AccountableFormInventory
@{
    DB_TOSSEntities TOSSDB = new DB_TOSSEntities();
}
@using TOSS_UPGRADE.Models;

<label>Accountable Form</label>
@Html.DropDownListFor(m => m.AccountableFormInvtID, Model.AccountableFormInvtList, new { @class = "form-control select2", id = "AFIDDDCT", onchange = "LoadQTYVal()" })





<script>
    $(function () {
        LoadQTYVal();
    })
    function LoadQTYVal() {
        var AccFormDD = $("#AFIDDDCT").val();
        $.ajax({
            url: "/FileMaintenanceAccountableForm/GetQTYVal",
            data: { AccFormDD: AccFormDD },
            success: function (data) {
                $("#txtORInvQuantityValueCT").val(data);
            }

        })
    }

</script>