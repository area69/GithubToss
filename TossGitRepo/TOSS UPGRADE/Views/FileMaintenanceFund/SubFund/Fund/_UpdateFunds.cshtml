

@model TOSS_UPGRADE.Models.FM_Funds.FM_Fund
@{
    DB_TOSSEntities SISDB = new DB_TOSSEntities();
}
@using TOSS_UPGRADE.Models;

@using (Ajax.BeginForm("UpdateFunds2", "FileMaintenanceFund", new AjaxOptions { OnSuccess = "AngularGlobalAlertsCallingNoModalNoSuccessWithDoubleFunc('FMFundController','Get_FundTable2s','UpdateMessageBox')", OnFailure = "OnFailure()", }))
{

    @Html.HiddenFor(m => m.getFundcolumns.FundID, new { })

    <div class="row black">
        <div class="col-md-2"></div>
        <div class="col-md-5">
            <label class="mt-1"><b>Fund</b></label>
            @Html.TextBoxFor(m => m.getFundcolumns.FundName, new { @class = "form-control", type = "text", placeholder = "Enter Fund Name" })
        </div>
        <div class="col-md-3">
            <label class="mt-1"><b>Fund Code</b></label>
            @Html.TextBoxFor(m => m.getFundcolumns.FundCode, new { @class = "form-control", type = "text", placeholder = "Enter Fund Code" })
        </div>
    </div>
    <div class="row mt-1">
        <div class="col-md-8"></div>
        <div class="col-md-2">
            <button type="submit" class="btn bg-addbutton form-control black">
                <b>Add</b>
            </button>
        </div>
        <div class="col-md-2">
            <button type="reset" onclick="AngularGlobalEdit('FMFundController','Get_AddFund2s','')" class="btn bg-headerbc white form-control"><b>Cancel</b></button>
        </div>
    </div>
}
<script>
    $(function () {
        LoadDataTable();
        LoadPopOver();
    })
</script>