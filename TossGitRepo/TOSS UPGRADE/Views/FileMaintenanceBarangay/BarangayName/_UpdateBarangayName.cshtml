
@model TOSS_UPGRADE.Models.FM_Barangay.FM_Barangay_BarangayName
@{
    DB_TOSSEntities SISDB = new DB_TOSSEntities();
}
@using TOSS_UPGRADE.Models;

@using (Ajax.BeginForm("UpdateBarangayName", "FileMaintenanceBarangay", new AjaxOptions { OnSuccess = "AngularGlobalAlertsCallingNoModal('FMBarangayController','Get_BarangayNameTable','Successfully Saved')", OnFailure = "OnFailure()", }))
{
    <div class="row">
        <div class="col-md-2"></div>
        <div class="col-md-8">
            <div class="row">
                @Html.HiddenFor(m => m.getBarangayNamecolumns.BarangayID, new { })
                <div class="col-md-8 form-group">
                    <label><b>Barangay Name</b></label>
                    @Html.TextBoxFor(m => m.getBarangayNamecolumns.BarangayName, new { @class = "form-control", type = "text" })
                </div>
                <div class="col-md-2">
                    <label style="visibility:hidden">Description</label>
                    <button type="submit" class="btn bg-addbutton black form-control"><b>Update</b></button>
                </div>
                <div class="col-md-2">
                    <label style="visibility:hidden">Description</label>
                    <button type="reset" class="btn bg-headerbc white form-control" onclick="AngularGlobalEdit('FMBarangayController','Get_AddBarangayName','')"><b>Cancel</b></button>
                </div>
            </div>
        </div>
    </div>
}
<script>
        $(function () {
            LoadDataTable();
            LoadPopOver();
        })
</script>

