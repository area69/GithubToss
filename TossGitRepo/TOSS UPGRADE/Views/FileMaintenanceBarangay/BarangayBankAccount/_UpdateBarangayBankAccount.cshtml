@model TOSS_UPGRADE.Models.FM_Barangay.FM_Barangay_BarangayName
@{
    DB_TOSSEntities SISDB = new DB_TOSSEntities();
}
@using TOSS_UPGRADE.Models;
@using (Ajax.BeginForm("UpdateBarangayBankAccount", "FileMaintenanceBarangay", new AjaxOptions { OnSuccess = "AngularGlobalAlertsCallingNoModal('FMBarangayController','Get_BarangayBankAccountTable','Successfully Updated')", OnFailure = "OnFailure()", }))
{
    @Html.HiddenFor(m => m.getBarangayBankAccountcolumns.BarangayBankAccountID, new { })
    @Html.HiddenFor(m => m.BarangayTempID, new { id = "BarangayTempID" })
    @Html.HiddenFor(m => m.BankTempID, new { id = "BankTempID" })
    @Html.HiddenFor(m => m.AccountNumberTempID, new { id = "AccountNumberTempID" })
    <div class="row black">
        <div class="col-md-2 form-group" id="_DynamicBarangayNameID">
        </div>
        <div class="col-md-4 form-group" id="_DynamicBankID">
        </div>
        <div class="col-md-3" id="_DynamicAccountNo">
        </div>
        <div class="col-md-3">
            <div class="row">
                <div class="col-md-6 form-group">
                    <label style="visibility:hidden">Position</label>
                    <button class="btn bg-addbutton black form-control"><b>Update</b></button>
                </div>
                <div class="col-md-6 form-group">
                    <label style="visibility:hidden">Position</label>
                    <button type="reset" onclick="AngularGlobalEdit('FMBarangayController','Get_AddBarangayBankAccount','')" class="btn bg-headerbc white form-control"><b>Cancel</b></button>
                </div>
            </div>
        </div>
    </div>
}
<script>
    $(function () {
        LoadDataTable();
        LoadPopOver();
    })
</script>
